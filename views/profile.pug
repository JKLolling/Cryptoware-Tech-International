extends layout
block content
  div(calss="main")
    
    div(class="container")   
          img(src=`${pictures}`, alt="Profile Pictures")
          div(class="username-profile")           
            p(class="fullname")=firstName 
          div   
            p(class="fullname")=lastName  
          div(class="bio-con")  
            p(class="bio-profile")=biography
    div(class="con-2")
          div(class="profile-header")
              div(class="profile-img")
              div(class="profile-nav-info")   
                  h3(class="activity")  
                    button('submit')  activity #{activity}
    div(class="productBox") productBox 
        ul 1
        ul 2
        ul 3       

         